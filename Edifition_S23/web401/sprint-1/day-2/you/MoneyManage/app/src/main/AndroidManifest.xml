<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.parvin.moneymanager">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/f"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/f"
        android:supportsRtl="true"
        android:theme="@style/Theme.MoneyManager">
        <activity
            android:name="com.parvin.moneymanager.MainActivity"
            android:exported="false" />
        <activity
            android:name="com.parvin.moneymanager.EditActivity"
            android:exported="true" />
        <activity
            android:name="com.parvin.moneymanager.SplashActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>